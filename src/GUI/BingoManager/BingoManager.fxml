<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.BingoManager.BingoManager">
    <children>
        <TabPane VBox.vgrow="ALWAYS" fx:id="tabPane">
         <tabs>
            <Tab closable="false" text="Clientes" fx:id="clientsTab">
              <content>
                  <fx:include source="../ClientsView/ClientsView.fxml" VBox.vgrow="ALWAYS"/>
              </content>
            </Tab>
            <Tab closable="false" text="Bingos" fx:id="bingosTab">
              <content>
                <fx:include source="../BingosView/BingosView.fxml" VBox.vgrow="ALWAYS"/>
              </content>
            </Tab>
             <Tab closable="false" text="Jugar" fx:id="playTab">
                 <content>
                     <fx:include source="../BingoPlayView/BingoPlayView.fxml" fx:id="bingoPlayViewL" VBox.vgrow="ALWAYS"/>
                 </content>
             </Tab>
         </tabs></TabPane>

    </children>
</VBox>
