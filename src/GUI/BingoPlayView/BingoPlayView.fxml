<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox prefHeight="400.0" prefWidth="600.0" spacing="4" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.BingoPlayView.BingoPlayView" >
    <children>
        <TilePane fx:id="numbersGrid"/>
        <Label text="Ganadores:" />
        <fx:include fx:id="inventoryListView" source="../InventoryListView/InventoryListView.fxml" VBox.vgrow="ALWAYS" />
        <HBox prefHeight="100.0" prefWidth="200.0">
            <children>
                <Button mnemonicParsing="false" text="Terminar Juego" />
                <Region HBox.hgrow="ALWAYS" />
                <Button disable="true" mnemonicParsing="false" text="Mostrar Detalles" />
            </children>
        </HBox>
    </children>
    <padding>
        <Insets bottom="4" left="4" right="4" top="4" />
    </padding>
</VBox>
