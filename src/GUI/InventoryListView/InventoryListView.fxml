<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox prefHeight="400.0" prefWidth="600.0" spacing="4.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.InventoryListView.InventoryListView">
   <children>
      <HBox spacing="4.0">
         <children>
            <TextField fx:id="textField" HBox.hgrow="ALWAYS" />
            <Button fx:id="cancelButton" mnemonicParsing="false" text="Cancelar" />
         </children>
      </HBox>
      <HBox fx:id="extraFilterPane" />
      <TableView fx:id="tableView" prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS" />
      <HBox spacing="4.0">
         <children>
            <Button fx:id="addButton" mnemonicParsing="false" text="Agregar" />
            <Button fx:id="removeButton" mnemonicParsing="false" text="Eliminar" disable="true" />
         </children>
      </HBox>
   </children>
</VBox>
